{% extends "base.html.twig" %}

{% block body  %}

<table class="table">
    <tr>
        <th>name</th>
        <th>price</th>
        <th>category</th>
    </tr>
    {% for product in products %}
        <tr>
            <td>{{ product.name }}</td>
            <td>{{ product.price }}</td>
            <td>{{ product.category.name }}</td>
        </tr>
    {% endfor %}
</table>

{% endblock %}